class Solution {
    public int findNumbers(int[] nums) {
        int n = nums.length;
        int count =0;
        for(int i =0;i<n;i++){
            int cnt = (int)Math.log10(nums[i])+1;
            // while(nums[i]>0){
            //     cnt++;
            //     nums[i]/=10;
            // }
            if(cnt%2==0) count++;
        }
        return count;
    }
}
