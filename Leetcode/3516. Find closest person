class Solution {
    public int findClosest(int x, int y, int z) {
        int d1 = (Math.abs(z-x));
        int d2 = (Math.abs(z-y));

        if(d1 == d2){
            return 0; //Both reaches at same time
        }
        else if(d1<d2){
            return 1;  // person 1 reaches first 
        }
        else return 2;  // person 2 reaches first
    }
}
