class Solution {
    public int maximumWealth(int[][] accounts) {
        int sum = 0;
        
        for(int i =0;i<accounts.length;i++){
            int maxSum = 0;
            for(int j =0;j<accounts[i].length;j++){
                maxSum += accounts[i][j];
            }
            sum = Math.max(sum,maxSum);
            // if(maxSum > sum){
            //     sum = maxSum;
            // }
        }
        return sum;
    }
}
